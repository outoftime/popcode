    ],
    "keyword-spacing": [
      "warn",
      {
        "before": true,
        "after": true
      }
    ],
    "linebreak-style": [
      "warn",
      "unix"
    ],
    "max-len": [
      "warn",
      79,
      2,
      {
        "ignoreUrls": true
      }
    ],
    "new-cap": [
      "warn",
