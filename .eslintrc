{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "__dirname": true
  },
  "overrides": [
    {
      "env": {
        "browser": false,
        "es6": true,
        "node": true,
        "jest": true
      },
      "files": ["**/__tests__/*.js"],
      "rules": {
      },
      "plugins": [
        "import",
        "jest",
        "react",
        "promise",
        "private-props"
      ]
    }
  ],
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "react",
    "promise",
    "private-props"
  ],
  "rules": {
    "accessor-pairs": [
      "error"
    ],
    "array-bracket-newline": [
      "warn",
      {
        "multiline": true
      }
    ],
    "array-bracket-spacing": [
      "warn",
      "never"
    ],
    "array-element-newline": [
      "warn",
      "consistent"
    ],
    "arrow-body-style": [
      "warn",
      "as-needed"
    ],
    "arrow-parens": [
      "warn",
      "as-needed",
      {
        "requireForBlockBody": true
      }
    ],
    "arrow-spacing": [
      "warn"
    ],
    "block-spacing": [
      "warn",
      "always"
    ],
    "brace-style": [
      "warn",
      "1tbs"
    ],
    "camelcase": [
      "warn",
      {
        "properties": "never"
      }
    ],
    "comma-dangle": [
      "warn",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "always-multiline"
      }
    ],
    "comma-spacing": [
      "warn",
      {
        "before": false,
        "after": true
      }
    ],
    "complexity": [
      "warn"
    ],
    "computed-property-spacing": [
      "warn",
      "never"
    ],
    "consistent-return": [
      "warn"
    ],
    "consistent-this": [
      "warn"
    ],
    "constructor-super": [
      "error"
    ],
    "curly": [
      "warn"
    ],
    "dot-location": [
      "warn"
    ],
    "dot-notation": [
      "warn"
    ],
    "eol-last": [
      "warn"
    ],
    "eqeqeq": [
      "warn"
    ],
    "function-paren-newline": [
      "warn",
      "consistent"
    ],
    "func-names": [
      "warn",
      "as-needed"
    ],
    "func-style": [
      "warn",
      "declaration"
    ],
    "generator-star-spacing": [
